   "source": [
    "def generate_random_sequences(lenght):\n",
    "    import random\n",
    "    STR_KEY_GEN = 'ACGT'\n",
    "    return ''.join(random.choice(STR_KEY_GEN) for _ in xrange(lenght))"
   ]
  },
